{
  "name": "Pulse Server",
  "version": "1.0.0",
  "description": "Pulse Server",
  "main": "app.js",
  "nodemonConfig": {
    "ignore": [
      "**/*.test.ts",
      "**/*.spec.ts",
      ".git",
      "node_modules"
    ],
    "watch": [
      "src"
    ],
    "exec": "node -r tsconfig-paths/register -r ts-node/register ./src/index.ts",
    "ext": "ts, js"
  },
  "scripts": {
    "test": "jest --verbose --coverage",
    "dev": "nodemon",
    "start": "ts-node -r tsconfig-paths/register --transpile-only src/index.ts"
  },
  "author": "Julian Schubel",
  "license": "MIT",
  "dependencies": {
    "@types/cookie-parser": "^1.4.7",
    "@types/cookie-session": "^2.0.49",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/http-status-codes": "^1.2.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node-cron": "^3.0.11",
    "@types/uuid": "^10.0.0",
    "cookie-parser": "^1.4.7",
    "cookie-session": "^2.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "express-validator": "^7.1.0",
    "http-status-codes": "^2.3.0",
    "jsonschema": "^1.5.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.9.2",
    "puppeteer": "^23.1.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.9.1",
    "jest": "^29.7.0",
    "nodemon": "^3.1.0",
    "ts-jest": "^29.3.4",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
